<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="org.zico.mappers.MenuMapper">
  <insert id="create">
  insert into tb_menu (category,mname,minfo,imgname,imgpath,msize)
  values(#{category},#{mname},{minfo},{imgname},{imgpath},{m_size})
  </insert>
  
  </mapper>